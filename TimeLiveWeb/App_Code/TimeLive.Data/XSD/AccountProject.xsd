                      vueAccountExpenseEntryForReport.QuantityFieldCaption, vueAccountExpenseEntryForReport.AccountBaseCurrencyId, 
                      vueAccountExpenseEntryForReport.ExchangeRate, vueAccountExpenseEntryForReport.TaxAmount, vueAccountExpenseEntryForReport.NetAmount, 
                      vueAccountExpenseEntryForReport.Submitted, vueAccountExpenseEntryForReport.TimeSheetApprovalTypeId, 
                      vueAccountExpenseEntryForReport.ExpenseApprovalTypeId, vueAccountExpenseEntryForReport.ProjectManagerEmployeeId, 
                      vueAccountExpenseEntryForReport.TimeSheetApprovalPathId, vueAccountExpenseEntryForReport.EMailAddress, 
                      vueAccountExpenseEntryForReport.DepartmentName, vueAccountExpenseEntryForReport.AccountLocation, vueAccountExpenseEntryForReport.AccountTaxZone, 
