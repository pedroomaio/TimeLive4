  <data name="Detail.Scripts.OnBeforePrint" xml:space="preserve">
    <value>Imports Microsoft.VisualBasic

Dim DetailExchangeRate as Double
Dim DetailOriginalExchangeRate as Double

Dim DetailNetAmount as Double
Dim DetailTaxAmount as Double
Dim DetailAmount as Double

Dim TotalNetAmount as Double
Dim TotalTaxAmount as Double
Dim TotalAmount as Double

Dim GroupNetAmount as Double
Dim GroupTaxAmount as Double
Dim GroupAmount as Double

Dim GrandNetAmount as Double
Dim GrandTaxAmount as Double
Dim GrandAmount as Double


Private Sub OnBeforePrint(ByVal sender As Object, ByVal e As System.Drawing.Printing.PrintEventArgs)

DetailExchangeRate = CType(lblExchangeRate.Text,Double)
DetailOriginalExchangeRate = CType(GetCurrentColumnValue("ExchangeRate"),Double)

DetailNetAmount = IIF(isdbnull(CType(GetCurrentColumnValue("NetAmount"),Double)),0,CType(GetCurrentColumnValue("NetAmount"),Double))
DetailTaxAmount = IIF(isdbnull(CType(GetCurrentColumnValue("TaxAmount"),Double)),0,CType(GetCurrentColumnValue("TaxAmount"),Double))
DetailAmount = IIF(Isdbnull(CType(GetCurrentColumnValue("Amount"),Double)),0,CType(GetCurrentColumnValue("Amount"),Double))


'' For Detail Total

If DetailNetAmount = 0 Then
TotalNetAmount = 0
Else
TotalNetAmount = (DetailNetAmount/DetailOriginalExchangeRate) * DetailExchangeRate
End If

If DetailTaxAmount = 0 Then
TotalTaxAmount = 0
Else
TotalTaxAmount = (DetailTaxAmount/DetailOriginalExchangeRate) * DetailExchangeRate
End If

If DetailAmount = 0 Then
TotalAmount = 0
Else
TotalAmount = (DetailAmount/DetailOriginalExchangeRate) * DetailExchangeRate
End If

'lblNetAmount.Text = String.Format("{0:n2}", Convert.ToString(TotalNetAmount))
'lblTaxAmount.Text = String.Format("{0:n2}", Convert.ToString(TotalTaxAmount))
'lblAmount.Text = String.Format("{0:n2}", Convert.ToString(TotalAmount))

lblNetAmount.Text = String.Format("{0:n2}", TotalNetAmount)
lblTaxAmount.Text = String.Format("{0:n2}", TotalTaxAmount)
lblAmount.Text = String.Format("{0:n2}", TotalAmount)

''  For Group Total

GroupNetAmount += Math.Round(TotalNetAmount,2)
GroupTaxAmount += Math.Round(TotalTaxAmount,2)
GroupAmount += Math.Round(TotalAmount,2)

'lblGroupNetAmount.Text = Convert.ToString(GroupNetAmount)
'lblGroupTaxAmount.Text = Convert.ToString(GroupTaxAmount)
'lblGroupAmount.Text = Convert.ToString(GroupAmount)

lblGroupNetAmount.Text = String.Format("{0:n2}", GroupNetAmount)
lblGroupTaxAmount.Text = String.Format("{0:n2}", GroupTaxAmount)
lblGroupAmount.Text = String.Format("{0:n2}", GroupAmount)


'' For Grand Total

'GrandNetAmount += Math.Round(GroupNetAmount,2)
'GrandTaxAmount += Math.Round(GroupTaxAmount,2)
'GrandAmount += Math.Round(GroupAmount,2)

GrandNetAmount += CType(lblNetAmount.Text,Double)
GrandTaxAmount += CType(lblTaxAmount.Text,Double)
GrandAmount += CType(lblAmount.Text,Double)

'lblGrandNetAmount.Text = Convert.ToString(GrandNetAmount)
'lblGrandTaxAmount.Text = Convert.ToString(GrandTaxAmount)
'lblGrandAmount.Text = Convert.ToString(GrandAmount)

lblGrandNetAmount.Text = String.Format("{0:n2}", GrandNetAmount)
lblGrandTaxAmount.Text = String.Format("{0:n2}", GrandTaxAmount)
lblGrandAmount.Text = String.Format("{0:n2}", GrandAmount)

End Sub</value>
