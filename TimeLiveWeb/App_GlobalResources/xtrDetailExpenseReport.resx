  <data name="Detail.Scripts.OnBeforePrint" xml:space="preserve">
    <value>Imports Microsoft.VisualBasic

Dim DetailExchangeRate as Double
Dim DetailAmount as Double
Dim DetailTotalAmount as Double
Dim DetailOriginalExchangeRate as Double
Dim CurrencyCode as String
Dim total as double
dim GroupTotal as Double

Private Sub OnBeforePrint(ByVal sender As Object, ByVal e As System.Drawing.Printing.PrintEventArgs)

DetailExchangeRate = CType(xrLabel22.Text,Double)
DetailAmount = IIF(Isdbnull(CType(GetCurrentColumnValue("Amount"),Double)),0,CType(GetCurrentColumnValue("Amount"),Double))
DetailOriginalExchangeRate = IIF(Isdbnull(CType(GetCurrentColumnValue("ExchangeRate"),Double)),0,CType(GetCurrentColumnValue("ExchangeRate"),Double))
CurrencyCode = CType(GetCurrentColumnValue("CurrencyCode"),String)

'DetailExchangeRate = Math.Round(DetailExchangeRate,3)
'DetailAmount = DetailAmount

'DetailExchangeRate 'iif(Convert.To(DetailExchangeRate) = 0, 1, Convert.ToDouble(DetailExchangeRate))
If DetailAmount = 0 Then
total =  0
Else
total = Math.Round((DetailAmount/DetailOriginalExchangeRate)*DetailExchangeRate,2)
End If
xrLabel21.Text = CurrencyCode &amp; " " &amp; DetailAmount

xrLabel20.Text = lblCurrencyCode.Text &amp; " " &amp; String.Format("{0:n2}", total)

GroupTotal += Math.Round(total,2)

xrLabel23.Text = lblCurrencyCode.Text &amp; " " &amp; String.Format("{0:n2}", GroupTotal)






End Sub</value>
